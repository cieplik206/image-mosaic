{
    "name": "cieplik206/image-mosaic",
    "description": "Create beautiful image mosaics and collages with Laravel and Intervention Image v3",
    "keywords": [
        "cieplik206",
        "laravel",
        "image-mosaic",
        "collage",
        "intervention-image",
        "image-processing"
    ],
    "homepage": "https://github.com/cieplik206/image-mosaic",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Ciepliński",
            "email": "pawel@cieplinski.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "intervention/image-laravel": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.0.0",
        "pestphp/pest": "^3.0||^4.0",
        "pestphp/pest-plugin-arch": "^3.0||^4.0",
        "pestphp/pest-plugin-laravel": "^3.0||^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cieplik206\\ImageMosaic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cieplik206\\ImageMosaic\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cieplik206\\ImageMosaic\\ImageMosaicServiceProvider"
            ],
            "aliases": {
                "Mosaic": "Cieplik206\\ImageMosaic\\Facades\\Mosaic"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
